{
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2020",
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "outDir": "./lib",
    "noEmit": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
